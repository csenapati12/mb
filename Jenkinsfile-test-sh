node(){

job_mode="$JOB_NAME"
       stage("Checkout"){
       checkout changelog: false, poll: false, scm: [$class: 'GitSCM', branches: [[name: '*/develop']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[url: 'https://github.com/csenapati12/mb.git']]]
       }
   // if("$JOB_NAME" == "job-name-test"){
        stage("job-name-test"){
           echo "Job name is $JOB_NAME"  
           echo "new job name $job_mode"
          sh"""
          ls
           echo "$Release_Version $job_mode"
           chmod 777 test.sh
           ./test.sh $build_mode $Release_Version $job_mode
           """
        }            
   // }
   // if("$JOB_NAME" == "job-name-test1"){
        stage("job-name-test1"){
           echo "Job name is $JOB_NAME" 
          sh """
          ls
            chmod 777 test.sh
           ./test.sh $build_mode $Release_Version $job_mode            
           echo "$Release_Version $job_mode"
           """
        }           
 //  }
   
}
